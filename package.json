{
  "name": "lysterfield-lake",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-external": "vite --host",
    "build": "vite build",
    "build-staging": "vite build --mode staging",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "test": "vitest",
    "generate": "node scripts/generate-dreams.js",
    "deploy": "npm run generate && npm run build-staging && rclone sync dist r2:lysterfield-lake --progress --transfers 24 --exclude-from ~/.gitignore_global",
    "deploy-dreams": "rclone sync dreams r2:lysterfield-lake-dreams --progress --transfers 24 --exclude-from ~/.gitignore_global",
    "deploy-prod": "npm run generate && npm run build && wrangler pages deploy dist --project-name lysterfield-lake",
    "postinstall": "npm run generate"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.0.10",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@react-av/controls": "^0.0.5",
    "@react-av/core": "^0.0.4",
    "@react-av/sliders": "^0.0.4",
    "@react-spring/three": "^9.7.3",
    "@react-three/drei": "^9.79.3",
    "@react-three/fiber": "^8.13.5",
    "@types/lodash": "^4.14.196",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/three": "^0.154.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@uidotdev/usehooks": "^2.1.0",
    "@use-gesture/react": "^10.2.27",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "camera-controls": "^2.7.0",
    "eslint": "^8.45.0",
    "eslint-config-mdcs": "^5.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "lodash": "^4.17.21",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "suspend-react": "^0.1.3",
    "tailwindcss": "^3.3.3",
    "three": "^0.154.0",
    "turbo": "^1.10.9",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "vitest": "^0.33.0",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "vite-plugin-mkcert": "^1.16.0",
    "vite-plugin-remove-console": "^2.1.1",
    "wrangler": "^3.17.1"
  }
}
